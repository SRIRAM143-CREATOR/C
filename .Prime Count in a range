#include <stdio.h>

int main() {
     int l,r;
    
      scanf("%d %d",&l,&r);
      int count=0;
     for(int i=l;i<=r;i++){
          int flag=0;
          for(int j=2;j<i;j++){
            if(i%j==0){
                flag=1;
                break;
            }  
          }
          if(flag==0 &&i>1){
              count++;
              
          }
         
          }
    
         printf("%d\n",count);
    
         for(int i=l;i<=r;i++){
          int flag=0;
          for(int j=2;j<i;j++){
            if(i%j==0){
                flag=1;
                break;
            }  
          }
          if(flag==0 && i>1){
              printf("%d ",i);
              
          }
         
          }
    
}
    
    
    
